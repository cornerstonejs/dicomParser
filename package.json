{
  "name": "dicom-parser",
  "version": "1.8.12",
  "description": "Javascript parser for DICOM Part 10 data",
  "main": "dist/dicomParser.min.js",
  "module": "dist/dicomParser.min.js",
  "type": "module",
  "browser": {
    "zlib": false
  },
  "keywords": [
    "DICOM",
    "medical",
    "imaging"
  ],
  "author": "Chris Hafey",
  "homepage": "https://github.com/cornerstonejs/dicomParser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cornerstonejs/dicomParser/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cornerstonejs/dicomParser.git"
  },
  "scripts": {
    "build": "npm run version && npm run webpack && npm run copy:meteor && npm run doc:generate",
    "build:ci": "npm run webpack:prod && npm run webpack:dev && npm run copy:meteor",
    "clean": "npm run clean:dist && npm run clean:coverage",
    "clean:coverage": "shx rm -rf coverage",
    "clean:dist": "shx rm -rf dist",
    "clean:docs": "shx rm -rf documentation",
    "copy:meteor": "shx cp ./meteor/dicomParser/package.js ./dist/",
    "doc": "npm run doc:generate && npx opn-cli documentation/index.html",
    "doc:generate": "npm run clean:docs && jsdoc -c .jsdocrc",
    "eslint": "eslint -c .eslintrc.cjs src",
    "eslint-fix": "eslint -c .eslintrc.cjs --fix src",
    "eslint-quiet": "eslint -c .eslintrc.cjs --quiet src",
    "start": "npm run webpack",
    "start:dev": "webpack-dev-server --config ./config/webpack/webpack-dev.cjs",
    "test": "npm run test:node",
    "test:all": "npm run test:chrome && npm run test:firefox && npm run test:node",
    "test:chrome": "karma start config/karma/karma-chrome.cjs",
    "test:node": "c8 mocha && c8 report --reporter=text-lcov > coverage/lcov.info",
    "test:firefox": "karma start config/karma/karma-firefox.cjs",
    "test:watch": "karma start config/karma/karma-watch.cjs",
    "version": "node -p -e \"'export default \\'' + require('./package.json').version + '\\';'\" > src/version.js",
    "watch": "npm run webpack:watch",
    "webpack": "npm run clean:dist && npm run webpack:prod && npm run webpack:dev",
    "webpack:dev": "webpack --progress --config ./config/webpack/webpack-dev.cjs",
    "webpack:prod": "webpack --progress --config ./config/webpack/webpack-prod.cjs",
    "webpack:watch": "webpack --progress --watch  --config ./config/webpack/index.cjs"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.1.1",
    "c8": "^8.0.1",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "docdash": "^1.2.0",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.7",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.1",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^4.0.0",
    "lodash": "^4.17.13",
    "mocha": "^9.1.1",
    "opn-cli": "^5.0.0",
    "puppeteer": "^19.2.2",
    "rusha": "^0.8.14",
    "shx": "^0.3.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  }
}
